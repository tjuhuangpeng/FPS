<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.learning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IsNameActivity"></activity>
        <activity android:name=".testActivity"></activity>
        <activity android:name=".Error"></activity>
        <activity android:name=".flaotmenu"></activity>
        <activity android:name="activity.BitmapActivity"></activity>
        <activity android:name="activity.addReflection"></activity>
        <activity android:name="activity.reverseBitmap"></activity>
        <activity android:name="activity.rotate00"></activity>
        <activity android:name="activity.rotateXY"></activity>
        <activity android:name="activity.toGray"></activity>
        <activity
            android:configChanges="keyboardHidden|orientation|screenSize|layoutDirection"
            android:screenOrientation="portrait"
            android:name=".Videolist">
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

</manifest>